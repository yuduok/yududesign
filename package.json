{
  "name": "yududesign",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages",
    "packages/*",
    "docs"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docs:dev": "cd docs && pnpm dev",
    "docs:build": "cd docs && pnpm build",
    "build:components": "cd packages && pnpm build",
    "build:all": "pnpm build:components && pnpm docs:build",
    "dev:components": "cd packages && pnpm dev",
    "clean": "cd packages && rm -rf dist && pnpm -r --filter='@yududesign/*' exec rm -rf lib es dist",
    "install:all": "pnpm install"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "next": "15.4.4"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
